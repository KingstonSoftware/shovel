{
  vars: {
    local: {
      contents: "{util.moustache(fs.readFile(path.join(sys.scriptDir, 'templates/moustache-template.txt')))}",
    },
  },
  statements: [
    {
      assert: "FileExists",
      with: {
        file: "scratch/moustached.txt",
      },
    },
    {
      assert: "FileContains",
      with: {
        file: "scratch/moustached.txt",
        contents: "{vars.local.contents}",
        position: "all",
      },
    },
  ],
}
