{
  vars: {
    bashProfile: {
      local: true,
      value: "${fs.readFile(path.join(sys.SCRIPT_DIR,'/templates/bash-profile.sh'))}",
    },
  },
  assertions: [
    {
      assert: "FileContains",
      with: {
        path: "${env.HOME}/.bash_profile",
        contents: "${bashProfile}",
      },
    },
  ],
}
