{
  options: {
    description: "A script to reverse the basic script",
  },
  vars: {
    TEST_DIR_1: "octo-dir-1",
    TEST_DIR_2: "octo-dir-2",
    TEST_FILE_1: "octo-file-1",
  },
  assertions: [
    {
      assert: "DirectoryAbsent",
      description: "The first directory",
      with: {
        path: "scratch/${TEST_DIR_1}",
      },
    },
    {
      assert: "DirectoryAbsent",
      description: "The second directory",
      with: {
        path: "scratch/${TEST_DIR_2}",
      },
    },
    {
      assert: "FileAbsent",
      with: {
        path: "scratch/${TEST_FILE_1}",
      },
    },
  ],
}
