{
  "name": "@johnls/octopus",
  "private": false,
  "version": "1.5.0",
  "description": "Simple, agentless IT automation",
  "bin": {
    "octopus": "dist/octopus.js"
  },
  "main": "dist/index.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rm -rf dist/* && babel src -d dist -s --ignore **/*.test.js",
    "start": "babel-node src/octopus.js",
    "start:debug": "babel-node --inspect-brk src/octopus.js",
    "test": "jest --coverage",
    "test:debug": "babel-node --inspect-brk ./node_modules/jest-cli/bin/jest.js --runInBand",
    "deploy": "npm publish --access public"
  },
  "jest": {
    "automock": false,
    "testPathIgnorePatterns": [
      "dist/",
      "node_modules/",
      "scratch/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlyonsmith/octopus.git"
  },
  "keywords": [
    "package",
    "project",
    "build",
    "release",
    "test",
    "start"
  ],
  "author": "John Lyon-Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jlyonsmith/octopus/issues"
  },
  "homepage": "https://github.com/jlyonsmith/octopus#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "jest-cli": "^24.9.0"
  },
  "dependencies": {
    "@johnls/json5": "^2.3.0",
    "@johnls/readline-password": "^1.1.0",
    "@rauschma/stringio": "^1.4.0",
    "autobind-decorator": "^2.4.0",
    "await-timeout": "^0.6.0",
    "chalk": "^2.4.2",
    "child-process-promise": "^2.2.1",
    "command-exists": "^1.2.8",
    "fs-extra": "^8.1.0",
    "linux-os-info": "^2.0.0",
    "merge": "^1.2.1",
    "minimist": "^1.2.0",
    "node-fetch": "^2.6.0",
    "node-pty": "^0.9.0-beta28",
    "node-ssh": "^6.0.0",
    "readdirp": "^3.1.3",
    "tempy": "^0.3.0",
    "validate.js": "^0.13.1",
    "yauzl-promise": "^2.1.3",
    "yazl": "^2.5.1"
  }
}
