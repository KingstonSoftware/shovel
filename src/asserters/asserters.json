{
  "DirectoryExists": {
    "module": "DirectoryExists",
    "arguments": ["path!"],
    "remotable": true,
    "description": "Asserts that a directory exists. ",
    "dependencies": {
      "fs-extra": "^7.0.1"
    }
  },
  "DirectoryAbsent": {
    "module": "DirectoryAbsent",
    "arguments": ["path!"],
    "remotable": true,
    "description": "Asserts that a directory does not exist",
    "dependencies": {
      "fs-extra": "^7.0.1"
    }
  },
  "FileExists": {
    "module": "FileExists",
    "arguments": ["path!"],
    "remotable": true,
    "description": "Asserts that a file exists. If not write a placeholder out",
    "dependencies": {
      "fs-extra": "^7.0.1"
    }
  },
  "FileAbsent": {
    "module": "FileAbsent",
    "arguments": ["path!"],
    "remotable": true,
    "description": "Asserts that a file does not exist. ",
    "dependencies": {
      "fs-extra": "^7.0.1"
    }
  },
  "FileDownloaded": {
    "module": "FileDownloaded",
    "arguments": ["url!", "checksum!", "checksumType", "toFile!"],
    "remotable": true,
    "description": "Asserts that a file as been downloaded. ",
    "dependencies": {
      "fs-extra":"^7.0.1",
      "request": "^2.0.0"
    }
  },
  "FileUnzipped": {
    "module": "FileUnzipped",
    "arguments": ["zipFile!", "toDir!"],
    "remotable": true,
    "description": "Asserts that a file has been unzipped. ",
    "dependencies": {
      "fs-extra":"^7.0.1",
      "unzip": "^0.1.11"
    }

  },
  "FileCopied": {
    "module": "FileCopied",
    "arguments": ["fromPath!", "toPath!"],
    "remotable": true,
    "description": "Asserts that a file (or directory) has been copied. ",
    "dependencies": {
      "fs-extra":"^7.0.1"
    }
  },
  "FileModeSet": {
    "module": "FileModeSet",
    "arguments": ["path!", "mode!"],
    "remotable": true,
    "description": "Asserts that a file mode is set ",
    "dependencies": {
      "fs-extra":"^7.0.1"
    }
  },
  "CommandOutputs": {
    "module": "CommandOutputs",
    "arguments": ["command!", "regex!", "occurence"],
    "remotable": true,
    "description": "Execute a command and check for expected output on stdout",
    "dependencies": {
      "child_process":"^12.0.1"
    }

  }
}